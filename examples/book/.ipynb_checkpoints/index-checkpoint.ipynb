    "project:\n",
    "  type: book\n",
    "\n",
    "execute:\n",
    "  freeze: auto\n",
    "\n",
    "book:\n",
    "  title: \"Sample bookup-html book project\"\n",
    "  author: \"Jane Doe\"\n",
    "  date: today\n",
    "  downloads: [pdf, epub]\n",
    "  doi: \"10.XXX/XXX\"\n",
    "  url: https://example.com\n",
    "  repo-url: https://github.com/juba/bookup-html\n",
    "  repo-actions: edit\n",
    "  search: true\n",
    "  reader-mode: true\n",
    "  sidebar:\n",
    "    style: \"docked\"\n",
    "  chapters:\n",
    "    - index.qmd\n",
    "    - part: \"Content\"\n",
    "      chapters:\n",
    "        - sample.qmd\n",
    "        - summary.qmd\n",
    "  appendices:\n",
    "    - references.qmd\n",
    "\n",
    "bibliography: references.bib\n",
    "\n",
    "format:\n",
    "  bookup-html:\n",
    "        toc: true\n",
    "        toc-depth: 3\n",
    "---"
